# Production Environment Variables

# Backend API URL (change to your production backend URL)
VITE_API_BASE_URL=https://api.cravecart.com

# Environment
VITE_ENV=production

# API Configuration
VITE_API_TIMEOUT=15000

# Payment
VITE_STRIPE_PUBLIC_KEY=your_stripe_public_key_here

# Cloudinary (for image uploads)
VITE_CLOUDINARY_NAME=your_cloudinary_name
VITE_CLOUDINARY_UPLOAD_PRESET=your_upload_preset

# Feature Flags
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_REVIEWS=true
VITE_ENABLE_WISHLIST=true
VITE_ENABLE_PROMO_CODES=true
VITE_ENABLE_ANALYTICS=true

# Analytics (optional)
VITE_GA_TRACKING_ID=your_google_analytics_id
VITE_MIXPANEL_TOKEN=your_mixpanel_token

# App Configuration
VITE_APP_NAME=CraveCart
VITE_APP_VERSION=1.0.0
VITE_SUPPORT_EMAIL=support@cravecart.com
