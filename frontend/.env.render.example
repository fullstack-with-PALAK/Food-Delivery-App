# Frontend Environment Variables for Render
# Copy these to Render dashboard environment variables

# API Configuration
VITE_API_BASE_URL=https://cravecart-backend.onrender.com

# Environment
VITE_ENV=production

# Analytics (optional)
VITE_ENABLE_ANALYTICS=true

# Feature Flags
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_REVIEWS=true
VITE_ENABLE_WISHLIST=true
VITE_ENABLE_PROMO_CODES=true

# Stripe Public Key (optional)
VITE_STRIPE_PUBLIC_KEY=pk_test_your_key_here

# App Configuration
VITE_APP_NAME=CraveCart
VITE_APP_VERSION=1.0.0
VITE_SUPPORT_EMAIL=support@cravecart.com
